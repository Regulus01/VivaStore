import 'package:uuid/uuid.dart';

class Product {
  final String id = const Uuid().v4();
  final String name;
  final double price;
  final String imageUrl;
  final String details;
  final String category;

  Product(
      {required this.name,
      required this.price,
      required this.imageUrl,
      required this.details,
      required this.category});
}
